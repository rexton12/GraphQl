
generator db {
  provider             = "prisma-client-py"
  interface            = "asyncio"
  recursive_type_depth = 5
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String   @id @default(cuid())
  firstname  String
  lastname   String
  published  Boolean
  desc       String?
}

model admin {
  id         String   @id @default(cuid())
  username   String
  password   String
  desc       String?
}